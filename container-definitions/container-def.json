[
    {
      "name": "colortest",
      "image": "221109526298.dkr.ecr.us-east-1.amazonaws.com/colortest",
      "cpu": 2,
      "memory": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": { 
          "awslogs-group" : "/ecs/frontend-container",
          "awslogs-region": "us-east-1"
        }
      }
    }
]
